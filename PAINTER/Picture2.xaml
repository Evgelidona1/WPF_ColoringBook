<Page x:Class="PAINTER.Picture2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PAINTER"
      mc:Ignorable="d" 
      d:DesignHeight="800" d:DesignWidth="1200"
      Title="Picture2" Background="Lavender">

    <Grid>
        <Button Style="{DynamicResource RoundButtonTemplate}"
                x:Name="Return" Height="50" Width="60"
                HorizontalAlignment="Left"
                FontSize="20" FontFamily="Segoe UI Black"
                BorderThickness="0"
                Click="ReturnButtonClick" Margin="0,750,0,0">
            <Button.Background>
                <ImageBrush ImageSource="Assets/arrow.png"/>
            </Button.Background>
        </Button>
    </Grid>
</Page>
